            android:theme="@style/AppTheme" />
        <activity
            android:name=".Question4"
            android:label="@string/title_activity_question4"
            android:theme="@style/AppTheme" />
        <activity android:name=".finalScore" />
        <activity android:name=".Question5" />
        <activity android:name=".Question6"></activity>
    </application>

</manifest>